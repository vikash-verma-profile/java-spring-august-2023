import java.util.Scanner;

class Person {
	private String name;
	private int age;
	private String gender;
	
	public Person() {
		System.out.println("Hi I am called in person");
	}

	public Person(String name, int age, String gender) {
		this.name = name;
		this.age = age;
		this.gender = gender;
		System.out.println("Hi I am called");
	}
	
	
}

